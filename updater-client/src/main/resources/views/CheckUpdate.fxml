<?xml version="1.0" encoding="UTF-8"?>

<!--suppress ALL -->
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox spacing="20.0" style="-fx-background-color: white;" stylesheets="/assets/css/app.css"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
    </padding>
    <VBox VBox.vgrow="ALWAYS" alignment="CENTER" prefWidth="400" spacing="20">
        <Label VBox.vgrow="ALWAYS" fx:id="titleLabel" style="-fx-font-size: 16px;" text="正在检查更新..."
               wrapText="true"/>
        <ImageView fitWidth="100.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">
            <Image url="/assets/imgs/loadding.gif"/>
        </ImageView>
    </VBox>
    <HBox alignment="CENTER_RIGHT" spacing="20.0">
        <HBox alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
            <Label style="-fx-font-size: 12px; -fx-text-fill: #868686;" text="Power by: cxxwl96@sina.com"/>
        </HBox>
    </HBox>
</VBox>
